{
  "builds": [
      {
        "src": "scripts/generate-env.js",
        "use": "@vercel/node"
      },
      {
        "src": "angular.json",
        "use": "@angular/cli",
        "config": {
          "root": ".",
          "buildCommand": "ng build --configuration production"
        }
      }
    ],
  "headers": [
    {
      "source": "/(.*)\\.ttf",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1296000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1296000, immutable"
        }
      ]
    },
    {
      "source": "assets/background/(.*)\\.webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1296000, immutable"
        }
      ]
    },
    {
      "source": "assets/icon/(.*)\\.webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1296000, immutable"
        }
      ]
    }
    ]
}
