{
  "headers": [
    {
      "source": "/assets/background/(.*\\.webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1296000"
        }
      ]
    },
    {
      "source": "/assets/icon/(.*\\.webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1296000"
        }
      ]
    },
    {
      "source": "/(.*)\\.ttf",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1296000"
        }
      ]
    },
  ]
}
